{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"]
    },
    "preview": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "db:push": {
      "cache": false,
      "persistent": true
    }
  }
}
